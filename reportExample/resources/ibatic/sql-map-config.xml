<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"  
  "http://ibatis.apache.org/dtd/sql-map-config-2.dtd"> 
<sqlMapConfig>
	<settings
		cacheModelsEnabled="true"
		enhancementEnabled="true"
		lazyLoadingEnabled="true"
		maxRequests="320"
		maxSessions="100"
		maxTransactions="50"
		useStatementNamespaces="true"
	/>
	<!-- 登录SQL引入 -->
	<sqlMap resource="/ibatic/auth/login.xml"/>
	
	<!-- 成交价-月报 -->
	<sqlMap resource="/ibatic/app/monthly/tp/TpMonthly.xml"/>
	
	<!-- 成交价-城市 -->
	<sqlMap resource="/ibatic/app/cityPrice/tp/TpCity.xml"/>
	
	<!-- 型号利润 -->
	<sqlMap resource="/ibatic/app/profit/profit.xml"/>
	
	<!--新车快讯-->
	<sqlMap resource="/ibatic/app/carNews/newCarsFw.xml"/>
	<sqlMap resource="/ibatic/app/carNews/TerminalGlobal.xml"/>
	
	<!-- 常用对象管理 -->
	<sqlMap resource="/ibatic/app/backstageConfig/commonGroup.xml"/>
	
	<!-- 本竞品管理 -->
	<sqlMap resource="/ibatic/app/backstageConfig/originalAndCompetitor.xml"/>
	
	<!--销量查询-->
	<sqlMap resource="/ibatic/app/sale/saleFw.xml"/>
	
	<!--基期TP-->
	<sqlMap resource="/ibatic/app/basePeriodTPConfig/basePeriodTP.xml"/>
	
</sqlMapConfig> 